{
  "name"           : "web-frontend-boilerplate",
  "version"        : "0.0.0-semantically-released",
  "description"    : "A boilerplate for developing frontend UI application",
  "keywords"       : [
    "boilerplate",
    "frontend",
    "web-frontend",
    "frontend-ui"
  ],
  "homepage"       : "https://github.com/dwiyatci/web-frontend-boilerplate",
  "bugs"           : "https://github.com/dwiyatci/web-frontend-boilerplate/issues",
  "license"        : "SEE LICENSE IN LICENSE.txt",
  "author"         : "Glenn Dwiyatcita <dwiyatci@mail.com> (https://tiny.cc/dwiyatci)",
  "repository"     : {
    "type": "git",
    "url" : "https://github.com/dwiyatci/web-frontend-boilerplate.git"
  },
  "scripts"        : {
    "install:clean"   : "rimraf node_modules && npm install",
    "start"           : "webpack-dev-server --hot",
    "lint"            : "eslint --cache src",
    "clean"           : "rimraf dist",
    "build"           : "npm run lint && npm run clean && webpack --env.prod",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "commit"          : "git-cz",
    "update"          : "updtr"
  },
  "dependencies"   : {
    "bootstrap"    : "4.0.0-alpha.2",
    "font-awesome" : "4.6.3",
    "jquery"       : "3.1.0",
    "kendo-ui-core": "2016.2.607",
    "lodash"       : "4.13.1",
    "tether"       : "1.3.2"
  },
  "devDependencies": {
    "babel-eslint"                  : "6.1.0",
    "babel-loader"                  : "6.2.4",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-polyfill"                : "6.9.1",
    "babel-preset-es2015-webpack"   : "6.4.1",
    "babel-preset-stage-2"          : "6.11.0",
    "babel-register"                : "6.9.0",
    "bootstrap-loader"              : "1.0.10",
    "commitizen"                    : "2.8.2",
    "css-loader"                    : "0.23.1",
    "cz-conventional-changelog"     : "1.1.6",
    "eslint"                        : "3.0.1",
    "eslint-config-airbnb"          : "9.0.1",
    "eslint-import-resolver-webpack": "0.3.2",
    "eslint-plugin-import"          : "1.10.2",
    "eslint-plugin-jsx-a11y"        : "1.5.5",
    "eslint-plugin-react"           : "5.2.2",
    "extract-text-webpack-plugin"   : "1.0.1",
    "file-loader"                   : "0.9.0",
    "font-awesome-loader"           : "0.0.1",
    "html-loader"                   : "0.4.3",
    "html-webpack-plugin"           : "2.22.0",
    "imports-loader"                : "0.6.5",
    "json-loader"                   : "0.5.4",
    "less"                          : "2.7.1",
    "node-sass"                     : "3.8.0",
    "postcss"                       : "5.0.21",
    "postcss-loader"                : "0.9.1",
    "resolve-url-loader"            : "1.6.0",
    "rimraf"                        : "2.5.3",
    "sass-loader"                   : "4.0.0",
    "semantic-release"              : "4.3.5",
    "style-loader"                  : "0.13.1",
    "updtr"                         : "0.2.1",
    "url-loader"                    : "0.5.7",
    "webpack"                       : "2.1.0-beta.14",
    "webpack-dev-server"            : "2.1.0-beta.0"
  },
  "config"         : {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
