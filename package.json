{
  "name"           : "my-frontend-boilerplate",
  "version"        : "0.0.1",
  "description"    : "My boilerplate for developing frontend UI application",
  "license"        : "SEE LICENSE IN LICENSE.txt",
  "author"         : "Glenn Dwiyatcita (https://tiny.cc/dwiyatci)",
  "dependencies"   : {
    "bootstrap"           : "4.0.0-alpha.2",
    "font-awesome"        : "4.5.0",
    "font-awesome-webpack": "0.0.4",
    "jquery"              : "2.2.1",
    "kendo-ui-core"       : "1.0.2",
    "lodash"              : "4.5.1"
  },
  "devDependencies": {
    "babel-core"                 : "6.6.0",
    "babel-loader"               : "6.2.4",
    "babel-polyfill"             : "6.6.1",
    "babel-preset-es2015"        : "6.6.0",
    "babel-preset-stage-0"       : "6.5.0",
    "css-loader"                 : "0.23.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader"                : "0.8.5",
    "html-loader"                : "0.4.3",
    "html-webpack-plugin"        : "2.9.0",
    "imports-loader"             : "0.6.5",
    "jscs"                       : "2.10.1",
    "json-loader"                : "0.5.4",
    "less"                       : "2.6.0",
    "rimraf"                     : "2.5.2",
    "style-loader"               : "0.13.0",
    "updtr"                      : "0.1.7",
    "url-loader"                 : "0.5.7",
    "webpack"                    : "1.12.14",
    "webpack-dev-server"         : "1.14.1"
  },
  "scripts"        : {
    "setup"        : "npm install rimraf updtr webpack webpack-dev-server jscs -g",
    "install:clean": "rimraf node_modules && npm install",
    "clean"        : "rimraf dist",
    "webpack"      : "webpack",
    "watch"        : "webpack -w",
    "start"        : "webpack-dev-server --inline --hot",
    "lint"         : "jscs src",
    "build"        : "npm run lint && npm run clean && webpack --config webpack.prod.config.js",
    "update"       : "updtr"
  },
  "repository"     : {
    "type": "git",
    "url" : "https://github.com/dwiyatci/my-frontend-boilerplate"
  }
}
