{
  "name": "web-frontend-boilerplate",
  "version": "0.0.0-semantically-released",
  "description": "A boilerplate for developing frontend UI application",
  "keywords": [
    "boilerplate",
    "frontend",
    "web-frontend",
    "frontend-ui"
  ],
  "homepage": "https://github.com/dwiyatci/web-frontend-boilerplate",
  "bugs": "https://github.com/dwiyatci/web-frontend-boilerplate/issues",
  "license": "SEE LICENSE IN LICENSE.txt",
  "author": "Glenn Dwiyatcita <dwiyatci@mail.com> (https://tiny.cc/dwiyatci)",
  "repository": {
    "type": "git",
    "url": "https://github.com/dwiyatci/web-frontend-boilerplate.git"
  },
  "scripts": {
    "install:clean": "yarn upgrade || yarn install",
    "update": "yarn upgrade-interactive",
    "start": "webpack-dev-server",
    "webpack": "npm run clean && webpack",
    "build": "npm run clean && cross-env NODE_ENV=production webpack",
    "lint": "eslint --cache src",
    "clean": "rimraf assets",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "commit": "git-cz"
  },
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "bootstrap": "4.0.0-alpha.6",
    "font-awesome": "4.7.0",
    "jquery": "3.2.1",
    "lodash": "4.17.4",
    "tether": "1.4.0"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.4.0",
    "bootstrap-loader": "2.1.0",
    "commitizen": "2.9.6",
    "cross-env": "5.0.0",
    "css-loader": "0.28.1",
    "cz-conventional-changelog": "2.0.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.1",
    "eslint-plugin-react": "7.0.1",
    "exports-loader": "0.6.4",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "font-awesome-loader": "1.0.2",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "imports-loader": "0.7.1",
    "inline-manifest-webpack-plugin": "3.0.1",
    "json-loader": "0.5.4",
    "less": "2.7.2",
    "node-sass": "4.5.2",
    "postcss": "6.0.1",
    "postcss-load-config": "1.0.0",
    "postcss-loader": "1.2.1",
    "resolve-url-loader": "2.0.2",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.5",
    "semantic-release": "6.3.6",
    "style-loader": "0.17.0",
    "url-loader": "0.5.8",
    "webpack": "2.5.1",
    "webpack-dev-server": "2.4.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
