{
  "name"           : "web-frontend-boilerplate",
  "version"        : "0.0.1",
  "description"    : "A boilerplate for developing frontend UI application",
  "keywords"       : [
    "boilerplate",
    "frontend",
    "web-frontend",
    "frontend-ui"
  ],
  "homepage"       : "https://github.com/dwiyatci/web-frontend-boilerplate",
  "bugs"           : "https://github.com/dwiyatci/web-frontend-boilerplate/issues",
  "license"        : "SEE LICENSE IN LICENSE.txt",
  "author"         : "Glenn Dwiyatcita <dwiyatci@mail.com> (https://tiny.cc/dwiyatci)",
  "repository"     : {
    "type": "git",
    "url" : "https://github.com/dwiyatci/web-frontend-boilerplate.git"
  },
  "scripts"        : {
    "install:global"  : "npm install rimraf updtr webpack webpack-dev-server jscs -g",
    "install:clean"   : "rimraf node_modules && npm install",
    "clean"           : "rimraf dist",
    "webpack"         : "webpack",
    "watch"           : "webpack -w",
    "start"           : "webpack-dev-server --inline --hot",
    "jscs"            : "jscs src",
    "build"           : "npm run jscs && npm run clean && webpack --config webpack.prod.config.js",
    "update"          : "updtr",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "commit"          : "git-cz"
  },
  "dependencies"   : {
    "bootstrap"    : "4.0.0-alpha.2",
    "font-awesome" : "4.6.3",
    "jquery"       : "2.2.4",
    "kendo-ui-core": "2016.2.603",
    "lodash"       : "4.13.1",
    "tether"       : "1.3.2"
  },
  "devDependencies": {
    "babel-core"                    : "6.9.1",
    "babel-loader"                  : "6.2.4",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-polyfill"                : "6.9.1",
    "babel-preset-es2015"           : "6.9.0",
    "babel-preset-stage-2"          : "6.5.0",
    "bootstrap-loader"              : "1.0.10",
    "commitizen"                    : "2.8.2",
    "css-loader"                    : "0.23.1",
    "cz-conventional-changelog"     : "1.1.6",
    "extract-text-webpack-plugin"   : "1.0.1",
    "file-loader"                   : "0.8.5",
    "font-awesome-loader"           : "0.0.1",
    "html-loader"                   : "0.4.3",
    "html-webpack-plugin"           : "2.19.0",
    "imports-loader"                : "0.6.5",
    "jscs"                          : "3.0.4",
    "json-loader"                   : "0.5.4",
    "less"                          : "2.7.1",
    "node-sass"                     : "3.7.0",
    "postcss"                       : "5.0.21",
    "postcss-loader"                : "0.9.1",
    "resolve-url-loader"            : "1.4.3",
    "rimraf"                        : "2.5.2",
    "sass-loader"                   : "3.2.0",
    "semantic-release"              : "4.3.5",
    "style-loader"                  : "0.13.1",
    "updtr"                         : "0.1.15",
    "url-loader"                    : "0.5.7",
    "webpack"                       : "1.13.1",
    "webpack-dev-server"            : "1.14.1"
  },
  "config"         : {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
